<% content_for :head do -%>
  <script src="https://unpkg.com/vue/dist/vue.js"></script>
<% end %>
<div id="app">
  <h3>Grocery List</h3>
  <ul>
    <li v-for="grocery in groceries">
      {{grocery}}
    </li>
  </ul>
</div>
<script>
  new Vue({
    el: '#app',
    data: {
      groceries: ['Bread', 'Milk']
    }
  });
</script>